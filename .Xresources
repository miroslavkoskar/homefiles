! colors ---------------------------------------------------------------------
!black
*color0:            #000000
*color8:            #555555

!red
*color1:            #AA0000
*color9:            #DD5555

!green
*color2:            #00AA00
*color10:           #55DD55

!yellow
!*color3:            #AA5500
*color3:            #FDB436
!*color11:           #DDDD55
*color11:           #FEED6A

!blue
*color4:            #1122AA
*color12:           #5555DD

!magenta
*color5:            #AA00AA
*color13:           #DD55DD

!cyan
*color6:            #00AAAA
*color14:           #55DDDD

!white
*color7:            #C0C0C0
*color15:           #DDDDDD

! xterm ----------------------------------------------------------------------
XTerm.termName:         xterm-256color-fix-bsdel
XTerm*background:       #000000
XTerm*foreground:       #C0C0C0
XTerm*cursorColor:      #DDDDDD
XTerm*faceName:         xft:local_terminal_sized
XTerm*colorMode:        true
XTerm*dynamicColors:    true
XTerm*bellIsUrgent:     true
XTerm*bellOnReset:      false
XTerm*metaSendsEscape:  true
XTerm*saveLines:        8192
XTerm*utf8:             2
XTerm*printAttributes:  0
XTerm*printerCommand:   cat > $(mktemp -p ~/tmp xterm.XXXXXX)
XTerm.backarrowKeyIsErase:  true
XTerm*VT100.translations:   #override \n\
                            ~Shift ~Alt Ctrl <Key>H: string(\177) \n\
                            ~Shift <Key>Print: print() \n\
                            Shift <Key>Print: print-everything()

! urxvt ----------------------------------------------------------------------
URxvt.termName:         rxvt-unicode-256color-fix-bsdel
URxvt.background:       0
URxvt.foreground:       7
URxvt.cursorColor:      15
URxvt.cursorColor2:     1
URxvt.scrollColor:      gray20
URxvt.underlineColor:   #FDB436
URxvt.font:             xft:local_terminal_sized
URxvt.scrollstyle:      plain
URxvt.thickness:        5
URxvt.saveLines:        8192
URxvt.pointerBlank:     true
URxvt.urgentOnBell:     true
URxvt.iso14755:         false
URxvt.iso14755_52:      false
URxvt.cutchars:         '"()*,<>[]{}|\''
URxvt.print-pipe:       cat > $(mktemp -p ~/tmp urxvt.XXXXXX)

! ASCII BS  == 8   == \010 == ^H
! ASCII DEL == 127 == \177 == ^?

! BackSpace = BS
! C-BackSpace = DEL
! C-h = BS
!URxvt.backspacekey:         \010
!URxvt.keysym.C-BackSpace:   \177

! BackSpace = DEL
! C-BackSpace = BS
! C-h = DEL
URxvt.keysym.C-h:       \177
URxvt.keysym.A-C-h:     builtin:
URxvt.keysym.S-C-h:     builtin:

URxvt.perl-ext-common:  default,matcher,confirm-paste
URxvt.matcher.button:   1
URxvt.url-launcher:     firefox

! xcursor --------------------------------------------------------------------
Xcursor.theme:          Grounation
Xcursor.size:           22

! xft ------------------------------------------------------------------------
Xft.dpi:                96
!Xft.dpi:                118
Xft.antialias:          true
Xft.hinting:            true
Xft.hintstyle:          hintslight
Xft.autohint:           false
Xft.rgba:               rgb
Xft.lcdfilter:          lcddefault
