# vim: ft=muttrc

#set folder = $my_example
set from = '<email>'
set my_base = $my_example
set my_imap = $my_example_imap

# mailboxes
set spoolfile = $my_example_spoolfile
set record = $my_example_record
set postponed = $my_example_postponed
set my_mbarchived = $my_example_mbarchived
set my_mbtrashed = $my_example_mbtrashed

# other
set query_command = "goobook query '%s'"
set sendmail = /usr/bin/msmtp

macro index,pager A <pipe-message>goobook-add<enter> 'add sender as contact'
macro index,pager \ez '<shell-escape>pg ~/.msmtp.log<enter>' 'show msmtp log'
