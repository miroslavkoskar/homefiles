#!/bin/bash

set -e

event=$1
session=$XSESSION

startup() {
    case $session in
        main)
            apod-bg &
            xrun0 clementine &
            xrun0 skype &
            xrun0 workrave &
            xup &
            ;;
    esac
}

case $event in
    startup)
        startup
        ;;
esac

wait
