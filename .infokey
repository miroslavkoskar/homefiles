#info

l       forward-char
h       backward-char
0       beginning-of-line
$       end-of-line
w       forward-word
b       backward-word

\kd     next-line
\ku     prev-line
\kr     forward-char
\kl     backward-char

g       beginning-of-node
G       end-of-node
u       up-node
d       dir-node
^N      next-node
^P      prev-node
^O      history-node

^J      select-reference-this-line
^M      select-reference-this-line
^]      select-reference-this-line
)       move-to-next-xref
(       move-to-prev-xref
\       clear-search

#echo-area

^B      echo-area-beg-of-line
#\m0    echo-area-beg-of-line

#^E     echo-area-end-of-line
\me     echo-area-end-of-line
#\m$    echo-area-end-of-line

\mh     echo-area-backward
\ml     echo-area-forward
#\mb    echo-area-backward-word
\mH     echo-area-backward-word
#\mf    echo-area-forward-word
\mw     echo-area-forward-word
\mL     echo-area-forward-word

^U      echo-area-backward-kill-line
#^K     echo-area-kill-line

^W      echo-area-backward-kill-word
#\md    echo-area-kill-word
#\mX    echo-area-kill-word
#\mx    echo-area-delete

#var

active-link-style=yellow,underline,bold
automatic-footnotes=On
automatic-tiling=On
cursor-movement-scrolls=Off
hide-note-references=On
highlight-searches=On
link-style=white,bold
match-style=bgred,white,bold
nodeline=print
scroll-behavior=Page Only
scroll-step=3
